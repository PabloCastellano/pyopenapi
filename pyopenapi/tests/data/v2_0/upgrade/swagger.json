{
    "swagger":"2.0",
    "tags":[
      {
         "name":"pet",
         "description":"Everything about your Pets",
         "externalDocs":{
            "description":"Find out more",
            "url":"http://swagger.io"
         }
      }
    ],
    "paths":{
      "/p1":{
         "get":{
            "produces":[
                "application/json"
            ],
            "parameters":[
               {
                  "name":"status",
                  "in":"query",
                  "required":false,
                  "type":"array",
                  "items":{
                     "type":"string",
                     "enum":[
                        "available",
                        "pending",
                        "sold"
                     ]
                  },
                  "collectionFormat":"multi",
                  "default":"available"
               }
            ],
            "responses":{
               "200":{
                  "description":"successful operation",
                  "schema":{
                     "type":"array",
                     "items":{
                        "$ref":"#/definitions/pet"
                     }
                  }
               }
            }
         }
      },
      "/p2":{
          "get":{
              "responses":{
                  "200":{
                      "description":"test for header",
                      "headers":{
                          "X-TEST":{
                              "type":"array",
                              "items":{
                                  "type":"string"
                              },
                              "collectionFormat":"csv"
                          }
                      }
                  }
              }
          }
      }
    },
    "definitions":{
      "category":{
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64"
            },
            "name":{
               "type":"string"
            }
         },
         "xml":{
            "name":"Category"
         }
      },
      "tag":{
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64"
            },
            "name":{
               "type":"string"
            }
         },
         "xml":{
            "name":"Tag"
         }
      },
      "pet":{
         "required":[
            "name",
            "photoUrls"
         ],
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64"
            },
            "category":{
               "$ref":"#/definitions/category"
            },
            "name":{
               "type":"string",
               "example":"doggie"
            },
            "photoUrls":{
               "type":"array",
               "xml":{
                  "name":"photoUrl",
                  "wrapped":true
               },
               "items":{
                  "type":"string"
               }
            },
            "tags":{
               "type":"array",
               "xml":{
                  "name":"tag",
                  "wrapped":true
               },
               "items":{
                  "$ref":"#/definitions/tag"
               }
            },
            "status":{
               "type":"string",
               "description":"pet status in the store",
               "enum":[
                  "available",
                  "pending",
                  "sold"
               ]
            }
         },
         "xml":{
            "name":"Pet"
         }
      },
      "some_file":{
          "type":"file"
      }
    },
    "parameters":{
        "form_file":{
            "name":"form_file",
            "in":"formData",
            "type":"file"
        },
        "body_file":{
            "name":"body_file",
            "in":"body",
            "schema":{
                "type":"file"
            }
        },
        "body_file_ref":{
            "name":"body_file_ref",
            "in":"body",
            "schema":{
                "$ref":"#/definitions/some_file"
            }
        }
    },
    "securityDefinitions":{
      "petstore_auth":{
         "type":"oauth2",
         "authorizationUrl":"http://petstore.swagger.io/api/oauth/dialog",
         "flow":"implicit",
         "scopes":{
            "write:pets":"modify pets in your account",
            "read:pets":"read your pets"
         }
      }
    },
    "externalDocs":{
       "description":"Find out more about Swagger",
       "url":"http://swagger.io"
    }
 }
